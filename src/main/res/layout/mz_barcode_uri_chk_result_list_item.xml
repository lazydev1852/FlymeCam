<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="wrap_content">
    <TextView android:textSize="@dimen/mz_font_size_12sp" android:textColor="@color/mz_barcode_result_item_title_color" android:id="@+id/title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/mz_barcode_list_item_margin_top" android:layout_alignParentTop="true" android:layout_alignParentStart="true"/>
    <TextView android:textSize="@dimen/mz_font_size_16sp" android:textColor="@android:color/white" android:id="@+id/content" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/mz_barcode_list_item_margin_center" android:layout_marginBottom="@dimen/mz_barcode_list_item_margin_bottom" android:layout_below="@+id/title" android:lineSpacingExtra="6dp" android:fontFamily="sans-serif-medium"/>
    <TextView android:textColor="@android:color/white" android:layout_gravity="center_vertical" android:id="@+id/extra_content" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentEnd="true"/>
    <TextView android:textSize="14sp" android:textColor="#ffffff" android:ellipsize="end" android:id="@+id/url_chk_tv_insist_access" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/mz_barcode_insistaccess_text_margin_top" android:layout_marginBottom="@dimen/mz_barcode_insistaccess_text_margin_bottom" android:maxLines="1" android:layout_below="@+id/content" android:fontFamily="sans-serif-medium"/>
    <TextView android:textSize="14sp" android:textColor="#66ffffff" android:ellipsize="end" android:id="@+id/url_chk_tv_supporter" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/mz_barcode_insistaccess_text_margin_top" android:layout_marginBottom="@dimen/mz_barcode_insistaccess_text_margin_bottom" android:maxLines="1" android:layout_below="@+id/content" android:fontFamily="sans-serif-medium" android:layout_alignParentEnd="true"/>
    <ImageView android:id="@+id/list_item_divider" android:background="@color/mz_barcode_divider" android:layout_width="match_parent" android:layout_height="@dimen/mz_barcode_devider" android:layout_alignParentBottom="true" android:layout_alignParentStart="true"/>
</RelativeLayout>
